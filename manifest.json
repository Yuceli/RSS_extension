{
  "manifest_version": 2,
  
  "name": "Noticias Diario de Yucatán",
  "description": "Visualizar noticias del Diario de Yucatán.",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"]
      "js": ["jquery.js", "popup.js"]
    }
  ],
  "permissions": [ "storage", "http://brentheftye.mx" ],
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "RSS Diario de Yucatán"
  }
}